<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

    <menuitem name="menu-programs"
              title="Programmes" />

    <menuitem name="menu-list-programs"
              parent="menu-programs"
              title="Liste Programmes"
              action="program.all"/>

    <action-view name="program.all"
                 title="Programmes"
                 model="com.axelor.program.db.Program">
        <view type="grid" name="program-grid"/>
        <view type="form" name="program-form"/>
    </action-view>

</object-views>